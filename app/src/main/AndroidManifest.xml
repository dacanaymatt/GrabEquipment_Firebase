<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carlo.grabequipment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GrabEquipment_Firebase"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminHome" />
        <activity android:name=".Home" />
        <activity android:name=".Register" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".GetEquipment" />
        <activity android:name=".Request" />
        <activity android:name=".AddEquipment" />
        <activity android:name=".ViewEquipment" />
        <activity android:name=".AdminRequest" />
        <activity android:name=".AdminHistory" />
        <activity android:name=".EditEquipment"></activity>
    </application>

</manifest>